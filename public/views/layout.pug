doctype html
mixin link(href, text)
	a(href=href)= text

mixin list(...items)
	ul
		each item in items
			li item

html(charset="utf-8")
	head
		block head
			link(rel="stylesheet", type="text/css", href="stylesheets/style.css")
		block title
			title LocatIn | #{title}
	body
		block nav
		block content
			p Not yet implented.
		block foot
